{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "noEmit": true,
        "allowJs": false,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "ES2020"
        ]
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}
